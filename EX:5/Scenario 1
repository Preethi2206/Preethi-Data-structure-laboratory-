class BookNode:
    def __init__(self, isbn, title, author, price, stock):
        self.isbn = isbn
        self.title = title
        self.author = author
        self.price = price
        self.stock = stock
        self.left = None
        self.right = None
class BookBST:
    def __init__(self):
        self.root = None
    def insert(self, isbn, title, author, price, stock):
        def _insert(root, node):
            if not root:
                return node
            if isbn < root.isbn:
                root.left = _insert(root.left, node)
            else:
                root.right = _insert(root.right, node)
            return root
        new_book = BookNode(isbn, title, author, price, stock)
        self.root = _insert(self.root, new_book)

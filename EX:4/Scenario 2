def check_balanced_parentheses(code):
    stack = []
    pairs = {')': '(', '}': '{', ']': '['}
    for char in code:
        if char in "({[":
            stack.append(char)
        elif char in ")}]":
            if not stack or stack.pop() != pairs[char]:
                return "Syntax error: unmatched closing bracket"
    return "Balanced" if not stack else "Syntax error: unmatched opening bracket"
code = "if (x > 0) { return (x); }"
print(check_balanced_parentheses(code))  

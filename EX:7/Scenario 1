class Student:
    def __init__(self, student_id, name, age):
        self.student_id = student_id
        self.name = name
        self.age = age
def find_student(records, student_id):
    for student in records:
        if student.student_id == student_id:
            return student
    return None
students = [Student(101, "Alice", 20), Student(102, "Bob", 22), Student(103, "Charlie", 21)]
student = find_student(students, 102)
if student:
    print(student.name)  
else:
    print("Student not found.")

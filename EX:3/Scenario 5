class CustomerNode:
    def __init__(self, customer_name):
        self.customer_name = customer_name
        self.next = None
class BankQueue:
    def __init__(self):
        self.front = None
        self.rear = None
    def enqueue_customer(self, customer_name):
        new_customer = CustomerNode(customer_name)
        if not self.rear:
            self.front = self.rear = new_customer
        else:
            self.rear.next = new_customer
            self.rear = new_customer
        print(f"Customer '{customer_name}' added to the queue.")
    def dequeue_customer(self):
        if not self.front:
            print("No customers in the queue.")
            return None
        next_customer = self.front.customer_name
        self.front = self.front.next
        if not self.front:
            self.rear = None
        print(f"Serving customer '{next_customer}'.")
        return next_customer
    def display_queue(self):
        if not self.front:
            print("No customers in the queue.")
        else:
            print("Current bank queue:")
            current = self.front
            while current:
                print(f"- {current.customer_name}")
                current = current.next
bank_queue = BankQueue()
bank_queue.enqueue_customer("Alice")
bank_queue.enqueue_customer("Bob")
bank_queue.display_queue()
bank_queue.dequeue_customer()
bank_queue.display_queue()

class CallQueue:
    def __init__(self):
        self.queue = []
    def enqueue_call(self, call_id):
        self.queue.append(call_id)
        print(f"Call '{call_id}' added to the queue.")
    def dequeue_call(self):
        if not self.queue:
            print("No calls to answer.")
            return None
        next_call = self.queue.pop(0)
        print(f"Answering call '{next_call}'.")
        return next_call
    def display_queue(self):
        if not self.queue:
            print("No calls in the queue.")
        else:
            print("Current queue of calls:")
            for call in self.queue:
                print(f"- Call ID: {call}")
call_center = CallQueue()
call_center.enqueue_call("Call_001")
call_center.enqueue_call("Call_002")
call_center.display_queue()
call_center.dequeue_call()
call_center.display_queue()

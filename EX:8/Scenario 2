class ProductCatalog:
    def __init__(self):
        self.products = {}  
    def add_product(self, product_id, product_info):
        self.products[product_id] = product_info
        print(f"Product {product_id} added.")
    def get_product(self, product_id):
        if product_id in self.products:
            return self.products[product_id]
        else:
            print("Product not found.")
            return None
    def remove_product(self, product_id):
        if product_id in self.products:
            del self.products[product_id]
            print(f"Product {product_id} removed.")
        else:
            print("Product not found.")
catalog = ProductCatalog()
catalog.add_product(101, {"name": "Laptop", "price": 1200})
catalog.add_product(102, {"name": "Smartphone", "price": 800})
print(catalog.get_product(101))  
catalog.remove_product(102)      

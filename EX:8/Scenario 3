class ArticleManager:
    def __init__(self):
        self.articles = {}  
    def add_article(self, title, content):
        self.articles[title] = content
        print(f"Article '{title}' added.")
    def get_article(self, title):
        return self.articles.get(title, "Article not found.")
manager = ArticleManager()
manager.add_article("Python Basics", "This is an article about Python basics.")
manager.add_article("Data Structures", "This covers data structures in detail.")
print(manager.get_article("Python Basics"))  
print(manager.get_article("Unknown"))        
